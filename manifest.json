{

    "manifest_version": 2,
    "name": "Auto join zoom meeting",
    "version": "0.1",
    "author": "Nguyen Xuan Duy Anh; Nguyen Duy Khoi",


    "permissions": ["alarms"],

    "description": "Auto login zoom meeting by invitation url",
    "background": {
        "scripts": ["auto_zoom.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "thor.jpg",
        "default_popup": "auto_zoom.html"
    }


}
